<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/eaclogoicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/eaclogoicon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalActivity"
        tools:targetApi="31">
        <activity
            android:name=".SecondActivityJohnVel"
            android:exported="false" />
        <activity
            android:name=".navigationdrawer"
            android:exported="true"
            android:label="@string/title_activity_navigationdrawer">
            <intent-filter>
                <action android:name="com.example.finalactivity.navigationdrawer" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>